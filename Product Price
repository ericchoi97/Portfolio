def subtotal(a, b, c, d, e):
    return (a + b + c + d + e)

a = int(input('Enter 1st product price: '))
b = int(input('Enter 2nd product price: '))
c = int(input('Enter 3rd product price: '))
d = int(input('Enter 4th product price: '))
e = int(input('Enter 5th product price: '))

def total(subtotal):
    return ((a + b + c + d + e)*1.07)

print(f'Total is {total(subtotal)}')
